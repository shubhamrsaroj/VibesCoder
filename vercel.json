{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build",
          "buildCommand": "cd client && CI=false npm install && CI=false DISABLE_ESLINT_PLUGIN=true npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "client/$1"
      }
    ],
    "env": {
      "CI": "false",
      "DISABLE_ESLINT_PLUGIN": "true"
    }
  } 